{
  "name": "tourreviewai-workers",
  "version": "2.1.0",
  "description": "Scalable review processing workers for TourReviewAI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node start.js",
    "start:prod": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t tourreviewai-workers .",
    "docker:run": "docker run --env-file .env tourreviewai-workers"
  },
  "keywords": [
    "reviews",
    "tripadvisor",
    "processing",
    "workers"
  ],
  "author": "TourReviewAI",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "retry": "^0.13.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
